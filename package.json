{
    "name": "tensorflowjs-experiments",
    "version": "0.1.0",
    "description": "A repo to learn Tensorflowjs",
    "source": "src/index.html",
    "scripts": {
        "start": "rm -rf .parcel-cache && parcel src/*.html --public-url / --open",
        "build": "cross-env PARCEL_WORKER_BACKEND=process parcel build src/*.html --no-cache --public-url ./",
        "predeploy": "rm -rf dist && cross-env PARCEL_WORKER_BACKEND=process parcel build src/*.html --no-cache --public-url ./",
        "deploy": "gh-pages -d dist",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
